{
  "name": "preact-aurora-ui",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:npm": "sh build-npm-package.sh",
    "publish:npm": "sh publish-npm.sh",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:all": "sh build-and-copy.sh",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "preact": "10.26.9",
    "preact-iso": "^2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prachwal/preact-aurora-ui.git"
  },
  "homepage": "https://github.com/prachwal/preact-aurora-ui#readme",
  "bugs": {
    "url": "https://github.com/prachwal/preact-aurora-ui/issues"
  },
  "author": "Prachwal",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@preact/preset-vite": "2.10.1",
    "@storybook/addon-a11y": "^9.0.13",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/addon-vitest": "^9.0.13",
    "@storybook/preact-vite": "^9.0.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "sass-embedded": "^1.89.2",
    "storybook": "^9.0.13",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "^3.2.4"
  }
}
