{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.eol": "\n",
  "files.exclude": {
    "dist": false,
    "node_modules": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "scss.validate": true,
  "vitest.config": "config/vitest.config.ts",
  "vitest.workspaceConfig": "config/vitest.config.ts"
}
